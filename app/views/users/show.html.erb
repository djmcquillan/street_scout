<h2>Videos Uploaded by<br> <%=@user.name%></h2>
<!-- <a href="http://localhost:3000/videos/new" class="btn btn-default"><span class="glyphicon glyphicon-cloud-upload"></span> Upload New Video</a> -->
<%#= link_to "Upload Video", new_video_path %> 

<div class="video-container">
<%=current_user.videos.each do |v| %>
<div class="panel panel-default">
  <div class="panel-heading">
    <h3 class="panel-title"><%="Title:" + " " + v.title %></h3>
  </div>
  <div class="panel-body" style="max-width: 600; max-height:400;">
    <%="Description:" + " " + v.description%><br>
    <%= link_to 'Edit', edit_video_path(v) %>
	<%= link_to 'Delete', v, method: :delete, data: { confirm: 'Are you sure?' } %>
	Score: <%=v.score%><br>
	<%= upload_video v.url %>
  </div>
</div>
<%end%>
</div>



